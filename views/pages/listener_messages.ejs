<!DOCTYPE html>
<html lang="en">
    <head>

    </head>
    <body class="l_messages">
        <main>
            <div class="current-messages">
            <%- include('../partials/listener_navbar') %>
            <h2>CURRENT CONVERSATIONS</h2>
            <% info.forEach(function (object) { %>
                <a id=loginLink href="/user/listener/<%= user.id %>/messages/<%= object.id %>"</a><%= object.name %></a><br>
            <% }); %>
            </div>
            <br>
            <br>
            <div class = "message-friends">
            <h2>MESSAGE A FRIEND</h2>
            <% friends.forEach(function (object) { %>
                <a id=loginLink href="/user/listener/<%= user.id %>/messages/<%= object.id %>"</a><%= object.name %></a><br>
            <% }); %>
            </div>
        </main>
    </body>
</html>